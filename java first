import java.util.*;

class next {
    public static void main(String[] args) {
        Scanner v = new Scanner(System.in);
        int num = v.nextInt();
        for (int i = 0; i < num; i++) {
            for (int j = 0; j < num; j++) {
                System.out.print("*");
            }
            System.out.println();
        }
    }
}
